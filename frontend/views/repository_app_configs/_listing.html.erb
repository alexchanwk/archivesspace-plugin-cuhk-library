<table id="app_config_list" class="table table-bordered table-striped table-condensed table-search-results">
  <thead>
    <tr>
        <th><%= I18n.t("plugins.repository_app_configs.key") %></th>
        <th><%= I18n.t("plugins.repository_app_configs.json") %></th>
        <th class="actions"></th>
    </tr>
  </thead>
  <tbody>
    <% app_configs.each do |app_config| %>
    <tr>
        <td>
            <%= "AppConfig[#{app_config["config_key"]}]" %><br/><%= "['#{current_repo['repo_code'].downcase}']" %>
        </td>
        <td>
            <%= app_config["config_json"] %>
        </td>
        <td>
            <div class="btn-group">
                <%= link_to I18n.t("actions.edit"), {:controller => :repository_app_configs, :action => :edit, :id => JSONModel(:repository_app_config).id_for(app_config["uri"])}, :class => "btn btn-xs btn-primary" %>
                <%= link_to I18n.t("actions.delete"), {:controller => :repository_app_configs, :action => :delete, :id => JSONModel(:repository_app_config).id_for(app_config["uri"])}, :"data-confirmation" => true, :"data-title" => I18n.t("actions.delete_confirm_title", :title => app_config["config_key"]), :"data-message" => I18n.t("actions.delete_confirm_message"), :"data-confirm-btn-label" => "#{I18n.t("actions.delete")}", :"data-confirm-btn-class" => "btn-danger", :class => "btn btn-xs btn-danger", :"data-authenticity_token" => form_authenticity_token %>
            </div>
        </td>
    </tr>
    <% end %>
  </tbody>
</table>